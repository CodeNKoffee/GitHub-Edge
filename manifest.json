{
  "manifest_version": 3,
  "name": "GitHub Edge",
  "description": "Search your favourite developer from the comfort of your own tab",
  "version": "1.0.0.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}
