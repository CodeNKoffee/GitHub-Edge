{
  "manifest_version": 2,
  "name": "GitHub Connect",
  "description": "Search for the Developer you are looking for from the comfort of your open tab",
  "version": "0.1.0",
  "icons": {
    "16": "icon_16.png",
    "48": "icon_48.png",
    "128": "icon_128.png"
  },
  "browser_action": {
    "default_icon": "icon_48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab"
  ]
}